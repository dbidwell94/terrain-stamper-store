{
  "name": "stamp-terrain-store",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "serve": "ts-node -r tsconfig-paths/register src/index.ts",
    "develop": "nodemon",
    "postinstall": "husky install",
    "up": "sudo docker-compose up --build -d",
    "down": "sudo docker-compose down --remove-orphans --volumes",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
  },
  "prettier": {
    "trailingComma": "es5",
    "printWidth": 120,
    "tabWidth": 2
  },
  "huksy": {
    "hooks": {
      "pre-commit": "echo \"[Husky] pre-commit\""
    }
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "bcrypt": "^5.0.0",
    "dotenv": "^8.2.0",
    "fkill": "^7.1.0",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.1",
    "koa-body": "^4.2.0",
    "koa-bodyparser": "^4.3.0",
    "koa-jwt": "^4.0.0",
    "koa-router": "^10.0.0",
    "koa-send": "^5.0.1",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "remove": "^0.1.5",
    "typeorm": "^0.2.30",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/koa": "^2.11.7",
    "@types/koa__cors": "^3.0.2",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.4.1",
    "@types/node": "^14.14.22",
    "@types/uuid": "^8.3.0",
    "eslint": "^7.19.0",
    "husky": "^5.0.8",
    "nodemon": "^2.0.7",
    "pinst": "^2.1.4",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  }
}
